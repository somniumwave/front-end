{
  "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
  },
  "parser": "@babel/eslint-parser",
  "extends": [
      "eslint:recommended",
      "airbnb",
      "airbnb/hooks",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jsx-a11y/recommended"
  ],
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaFeatures": {
          "jsx": true
      },
      "ecmaVersion": 11,
      "requireConfigFile": false,,
      "sourceType": "module"
  },
  "settings": {
      "react": {
          "version": "detect"
      }
  },
  "plugins": [
      "react",
      "react-hooks"
  ],
  "rules": {
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": [
          1,
          {
              "extensions": [
                  ".js",
                  ".jsx"
              ]
          }
      ],
      "react/display-name": 1
  }
}